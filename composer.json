{
  "name": "nmullen/apiengine",
  "description": "A common base for consuming APIs",
  "keywords": [
    "nmullen",
    "apiengine",
    "API"
  ],
  "homepage": "https://github.com/nmullen/apiengine",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicholas Mullen",
      "email": "nicholas.mullen@bigpond.com",
      "homepage": "https://github.com/nmullen/",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nmullen/http.git"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "psr/log": "1.0.*@dev",
    "psr/http-message": "1.0.*@dev",
    "nmullen/http": "dev"
  },
  "require-dev": {
    "monolog/monolog": "1.12.0"
  },
  "autoload": {
    "psr-4": {
      "Nmullen\\ApiEngine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nmullen\\ApiEngine\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}